class Solution {
public:
    int maxArea(vector<int>& height) {
        int lp=0,rp=height.size()-1, ans=0;
        while(lp< rp){

           int  w= rp-lp;
           int ht = min(height[lp],height[rp]);
           int currwt= w* ht;
           ans= max( ans, currwt);

           if(height[lp]> height[rp]){
            rp--;
           }
           else{
            lp++;
           }

        }
        return ans;
    }
};
       
